<div class="col-12" id="au_section4_1" >
  <div class="row text-center ">
       {% set i = 0 %}
    {% for node in count %}
    <div class="col-xl-3 col-12">
      <div class="counter2 ch-{{ i }} mb-4" id="count-interne">

        
        <p class="count-text ">{{ node.service }}</p>
        <img src="/{{ base_path ~ directory }}/images/count-{{ i }}.jpg">
        <h2 class="timer count-title count-number" data-to="{{ node.value }}" data-speed="1500"></h2>
      </div>
    </div>
     {% set i = i + 1 %}
     {% endfor %}
  </div>

</div>