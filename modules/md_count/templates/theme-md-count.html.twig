<div class="col-12" id="au_section4_1" >
  <div class="row text-center  blog-slider-active-count">
       {% set i = 0 %}
    {% for node in count %}
    <div class="col-xl-2 col-12">
      <div class="counter2 ch-{{ i }}">

        {% set note_ttile = node.service %}
        <p class="count-text 1">{{ note_ttile|t }} </p>
        <img src="/{{ base_path ~ directory }}/images/count-{{ i }}.jpg">
        <h2 class="timer count-title count-number" data-to="{{ node.value }}" data-speed="1500"></h2>
      </div>
    </div>
     {% set i = i + 1 %}
     {% endfor %}
  </div>

</div>